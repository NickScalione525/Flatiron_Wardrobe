<%=render(partial: "application/errors", locals: {errors: @style.errors.full_messages})%>

<h3>Editing Style for <%=@style.outfit.name%></h3>

<%= form_for(@style) do |f|%>
  <%=f.label "Name"%>
  <%=f.number_field(:name)%>
  <%=f.label "Era"%>
  <%=f.text_field(:era)%>
  <%=f.submit("Save")%>
<%end%>